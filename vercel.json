{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" },
    { "src": "animations.js", "use": "@vercel/static" },
    { "src": "Images/*", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "x-vercel-toolbar", "value": "hidden" }
      ]
    }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact.js" },
    { "src": "/api/health", "dest": "/api/health.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}


